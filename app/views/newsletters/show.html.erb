<div id="map"></div>
<script>
  var map;
  function initMap() {
    var myLatLng = {lat: 42.3601, lng: -71.0589};
    map = new google.maps.Map(document.getElementById('map'), {
      center: myLatLng,
      zoom: 3
    });
  }
</script>
<script src="<%= @source %>" async defer></script>
<div id='app'></div>
